{
  "thywill": {
    "launch": {
      "port": 10080,
  	  "groupId": "node",
  	  "userId": "node"
    },
  	"adminInterface": {
  	  "ipAddresses": ["127.0.0.1"],
  	  "paths": {
  	    "prepareForShutdown": "/prepareForShutdown"
  	  },
      "port": 20080
  	}
  },
  
  "clientInterface": {
    "component": "socketIO",
    "basePath": "/echo",
    "encoding": "utf-8",
    "encryption": {
      "key": "change this key"
    },
    "minifyCss": false,
    "minifyJavascript": false,
    "namespace": "/echoNamespace",
    "socketClientConfig": {
      "resource": "echo/socket.io"
    },
    "socketConfig": {
      "global": {
	      "browser client minification": true,
	      "browser client etag": true,
	      "log level": 1,
	      "match origin protocol": true,
	      "resource": "/echo/socket.io",
   	    "transports": ["websocket"]
      },
      "production": {
        "log level": 1
      }
    }
  },
  
  "resourceManager": {
    "component": "inMemory"
  },
  
  "log": {
    "component": "console",
    "level": "debug"
  },
  
  "messageManager": {
    "component": "simple"
  },
  
  "minify": {
    "component": "ugly",
    "cssBasePath": "/echo/css",
    "jsBasePath": "/echo/js"
  },
  
  "template": {
  	"component": "ejs"
  },
  
  "resources": {
  	"component": "useResourceManager"
  }
}
