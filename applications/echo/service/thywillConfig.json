{
  "thywill": {
    "launch": {
      "port": 10080,
  	  "groupId": "node",
  	  "userId": "node"
    },
  	"prepareForShutdown": {
      "port": 20080,
  	  "path": "/echo/prepareForShutdown"
  	}
  },
  
  "clientInterface": {
    "component": "socketIO",
    "basePath": "/echo",
    "clientConfig": {
      "resource": "echo/socket.io"
    },
    "encoding": "utf-8",
    "encryption": {
      "key": "change this key"
    },
    "minifyCss": true,
    "minifyJavascript": true,
    "namespace": "/echoNamespace",
    "socketConfig": {
      "global": {
	      "browser client minification": true,
	      "browser client etag": true,
	      "log level": 3,
	      "match origin protocol": true,
	      "resource": "/echo/socket.io",
   	    "transports": ["websocket"]
      },
      "production": {
        "log level": 1
      }
    }
  },
  
  "datastore": {
    "component": "memory"
  },
  
  "log": {
    "component": "console",
    "level": "debug"
  },
  
  "minify": {
    "component": "ugly",
    "cssBasePath": "/echo/css",
    "jsBasePath": "/echo/js"
  },
  
  "template": {
  	"component": "ejs"
  },
  
  "resources": {
  	"component": "useDatastore"
  }
}
