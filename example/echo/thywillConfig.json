{
  "thywill": {
  	"prepareForShutdown": {
      "port": 10080,
  	  "path": "/thywill/prepareForShutdown"
  	}
  },
  
  "clientInterface": {
    "component": "socketIO",
    "encoding": "utf-8",
    "basePath": "/thywill"
    "namespace": "/thywillNamespace"
  },
 
  "appInterface": {
    "component": "local"
  },
  
  "datastore": {
    "component": "memory"
  },
  
  "log": {
    "component": "console",
    "level": "debug"
  },
  
  "template": {
  	"component": "ejs"
  },
  
  "resources": {
  	"component": "useDatastore"
  }
}
